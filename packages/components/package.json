{
  "name": "@labkey/components",
  "version": "0.1.0-fb-packageConsolidation.12",
  "description": "Components, models, actions, and utility functions for LabKey applications and pages",
  "main": "dist/components.js",
  "module": "dist/components.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "nps build",
    "clean": "nps clean",
    "cleanAll": "nps cleanAll",
    "prepare": "yarn build",
    "storybook": "start-storybook -p 9001 -s ./static -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o .storybook-out",
    "test": "cross-env NODE_ENV=test jest",
    "test_teamcity": "cross-env NODE_ENV=test jest --testResultsProcessor=jest-teamcity-reporter && yarn run build-storybook",
    "watch:build": "watch 'yarn build' ./src",
    "watch": "npm run watch:build",
    "build:docs": "npm run clean:docs && typedoc --out ../../docs --tsconfig tsconfig.base.json --options typedoc.js . && touch ../../docs/.nojekyll",
    "clean:docs": "rimraf ../../docs",
    "deploy:docs": "npm run build:docs && gh-pages -t -d ../../docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LabKey/glass-components.git"
  },
  "author": "LabKey",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://www.labkey.org/project/home/Developer/issues/begin.view?"
  },
  "publishConfig": {
    "registry": "https://artifactory.labkey.com/artifactory/api/npm/libs-client"
  },
  "homepage": "https://github.com/LabKey/glass-components#readme",
