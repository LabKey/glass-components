    "cleanAll": "nps cleanAll",
    "prepublishOnly": "yarn build",
    "test": "cross-env NODE_ENV=test jest --maxWorkers=6",
    "watch:build": "watch 'yarn build' ./src",
    "watch": "npm run watch:build",
    "lint": "eslint",
