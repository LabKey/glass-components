{
  "name": "@glass/domainproperties",
  "version": "0.0.6",
  "description": "Domain property related components for LabKey domains.",
  "main": "dist/domainproperties.cjs.js",
  "module": "dist/domainproperties.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "nps build",
    "clean": "nps clean",
    "cleanAll": "nps cleanAll",
    "test": "cross-env NODE_ENV=test jest",
    "test_teamcity": "cross-env NODE_ENV=test jest --testResultsProcessor=jest-teamcity-reporter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LabKey/glass-components.git"
  },
  "author": "Cory Nathe",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/LabKey/glass-components/issues"
  },
  "publishConfig": {
    "registry": "https://artifactory.labkey.com/artifactory/api/npm/libs-client"
  },
  "homepage": "https://github.com/LabKey/glass-components#readme",
  "dependencies": {
    "@glass/base": "0.1.6"
  },
  "jest": {
    "globals": {
      "LABKEY": {}
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/../../test/styleMock.ts"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/../../test/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
