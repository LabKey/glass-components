{
  "name": "@glass/models",
  "version": "0.0.3",
  "description": "Shared models for LabKey components",
  "main": "dist/models.cjs.js",
  "module": "dist/models.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "nps build",
    "clean": "nps clean",
    "cleanAll": "nps cleanAll",
    "test": "cross-env NODE_ENV=test jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LabKey/glass-components.git"
  },
  "author": "Cory Nathe",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/LabKey/glass-components/issues"
  },
  "publishConfig": {
    "registry": "https://artifactory.labkey.com/artifactory/api/npm/libs-client"
  },
  "homepage": "https://github.com/LabKey/glass-components#readme",
  "dependencies": {
    "@glass/utils": "0.0.6"
  },
  "peerDependencies": {
    "immutable": "^3.8.2"
  },
  "jest": {
    "globals": {
      "LABKEY": {}
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/../../test/styleMock.ts"
    },
    "setupFilesAfterEnv": ["<rootDir>/../../test/jest.setup.ts"],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
