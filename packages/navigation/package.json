{
  "name": "@glass/navigation",
  "version": "0.3.6-fb-add-assay-wc-workflow.2",
  "description": "Navigation elements and actions for an application and its pages",
  "main": "dist/navigation.cjs.js",
  "module": "dist/navigation.es.js",
  "style": "dist/navigation.css",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "nps build",
    "clean": "nps clean",
    "cleanAll": "nps cleanAll",
    "prepublish": "yarn build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "cross-env NODE_ENV=test jest",
    "test_teamcity": "cross-env NODE_ENV=test jest --testResultsProcessor=jest-teamcity-reporter",
    "watch": "watch 'yarn build' ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LabKey/glass-components.git"
  },
  "author": "Susan Hert",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/LabKey/glass-components/issues"
  },
  "publishConfig": {
    "registry": "https://artifactory.labkey.com/artifactory/api/npm/libs-client"
  },
  "homepage": "https://github.com/LabKey/glass-components#readme",
  "dependencies": {
    "@glass/base": "0.10.0-fb-add-assay-qc-workflow.2",
    "@types/react-router": "3.0.13",
    "prop-types": "15.7.2",
    "react-router": "3.2.1"
  },
  "devDependencies": {
    "@types/react-router": "3.0.13"
  },
  "peerDependencies": {
    "immutable": "^3.8.2",
    "react": "^16.0",
    "react-dom": "^16.0"
  },
  "jest": {
    "globals": {
      "LABKEY": {}
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/../../test/styleMock.ts"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/../../test/jest.setup.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "gitHead": "3e6e7df581308f3fb362fe41c34b8154321b2fc1"
}
