{
  "name": "@glass/querygrid",
  "version": "0.1.7-alpha.1",
  "description": "Query Grid for LabKey schema/query data views",
  "main": "dist/querygrid.cjs.js",
  "module": "dist/querygrid.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "nps build",
    "clean": "nps clean",
    "cleanAll": "nps cleanAll",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "cross-env NODE_ENV=test jest",
    "test_teamcity": "cross-env NODE_ENV=test jest --testResultsProcessor=jest-teamcity-reporter",
    "watch": "watch 'yarn build' ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LabKey/glass-components.git"
  },
  "author": "Cory Nathe",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/LabKey/glass-components/issues"
  },
  "publishConfig": {
    "registry": "https://artifactory.labkey.com/artifactory/api/npm/libs-client"
  },
  "homepage": "https://github.com/LabKey/glass-components#readme",
  "dependencies": {
    "@glass/base": "0.1.5",
    "@glass/omnibox": "0.1.5",
    "bootstrap-sass": "3.4.0",
    "history": "4.7.2",
    "normalizr": "3.3.0"
  },
  "devDependencies": {
    "xhr-mock": "2.4.1"
  },
  "peerDependencies": {
    "immutable": "^3.8.2",
    "jquery": "^3.0",
    "react": "^16.0",
    "react-dom": "^16.0",
    "reactn": "^1.0.0"
  },
  "jest": {
    "globals": {
      "LABKEY": {}
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/../../test/styleMock.ts"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/../../test/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
