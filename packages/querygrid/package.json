{
  "name": "@glass/querygrid",
  "version": "0.0.3",
  "description": "Query Grid for LabKey schema/query data views",
  "main": "dist/querygrid.cjs.js",
  "module": "dist/querygrid.es.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "nps build",
    "clean": "nps clean",
    "cleanAll": "nps cleanAll",
    "test": "cross-env NODE_ENV=test jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LabKey/glass-components.git"
  },
  "author": "Cory Nathe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LabKey/glass-components/issues"
  },
  "publishConfig": {
    "registry": "https://artifactory.labkey.com/artifactory/api/npm/libs-client"
  },
  "homepage": "https://github.com/LabKey/glass-components#readme",
  "dependencies": {
    "@glass/grid": "0.0.2",
    "@labkey/api": "0.0.9",
    "history": "3.2.1",
    "normalizr": "3.3.0",
    "react-bootstrap": "0.32.4"
  },
  "peerDependencies": {
    "immutable": "^3.8.2",
    "react": "^16.0",
    "react-dom": "^16.0",
    "reactn": "0.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/../../test/styleMock.ts"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/../../test/jest.setup.ts",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
