{
  "name": "@glass/querygrid",
  "version": "0.3.0-fb-sample-details.8",
  "description": "Query Grid for LabKey schema/query data views",
  "main": "dist/querygrid.cjs.js",
  "module": "dist/querygrid.es.js",
  "types": "dist/index.d.ts",
  "config": {
    "application_dir": "/path/to/application/node_modules/\\@glass/querygrid/dist"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "nps build",
    "clean": "nps clean",
    "cleanAll": "nps cleanAll",
    "prepublish": "yarn build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "cross-env NODE_ENV=test jest",
    "test_teamcity": "cross-env NODE_ENV=test jest --testResultsProcessor=jest-teamcity-reporter",
    "watch:build": "watch 'yarn build' ./src",
    "watch:distCopy": "cpx --watch -v './dist/**/*' $npm_package_config_application_dir",
    "watch:buildAndCopy": "npm run watch:build & npm run watch:distCopy",
    "watch": "npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LabKey/glass-components.git"
  },
  "author": "Cory Nathe",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/LabKey/glass-components/issues"
  },
  "publishConfig": {
    "registry": "https://artifactory.labkey.com/artifactory/api/npm/libs-client"
  },
  "homepage": "https://github.com/LabKey/glass-components#readme",
  "dependencies": {
    "@glass/base": "0.2.0-fb-sample-details.4",
    "@glass/omnibox": "0.2.0-fb-sample-details.4",
    "@types/prop-types": "15.5.5",
    "@types/react-router": "3.0.13",
    "@types/react-select": "1.0.57",
    "bootstrap-sass": "3.4.0",
    "formsy-react": "1.1.5",
    "formsy-react-components": "1.0.0",
    "history": "4.7.2",
    "normalizr": "3.3.0",
    "prop-types": "15.7.2",
    "react-router": "3.2.1",
    "react-select": "1.0.0-rc.10"
  },
  "devDependencies": {
    "xhr-mock": "2.4.1"
  },
  "peerDependencies": {
    "immutable": "^3.8.2",
    "jquery": "^3.0",
    "react": "^16.0",
    "react-dom": "^16.0",
    "reactn": "^1.0.0"
  },
  "jest": {
    "globals": {
      "LABKEY": {
        "contextPath": "/labkey"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/../../test/styleMock.ts"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/../../test/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
