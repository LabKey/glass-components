  },
  "homepage": "https://github.com/LabKey/glass-components#readme",
  "dependencies": {
    "@glass/base": "0.3.0-fb-package-updates.3"
  },
  "jest": {
    "globals": {
      "LABKEY": {}
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/../../test/styleMock.ts"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/../../test/jest.setup.ts"
