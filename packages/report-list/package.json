    "dist/"
  ],
  "scripts": {
    "build": "nps build",
    "clean": "nps clean",
    "cleanAll": "nps cleanAll",
    "prepare": "yarn build",
    "storybook": "start-storybook -p 9001 -s ./static -c .storybook",
    "test": "cross-env NODE_ENV=test jest",
    "watch": "watch 'yarn build' ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LabKey/glass-components.git"
