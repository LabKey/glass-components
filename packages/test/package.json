    "jest": "29.6.1",
    "jest-cli": "29.6.1",
    "jest-environment-jsdom": "29.6.1",
    "jest-teamcity-reporter": "0.9.0",
    "rimraf": "6.0.1",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "typescript": "5.1.6",
    "webpack": "5.88.1",
    "webpack-cli": "5.1.4"
  },
  "peerDependencies": {
    "@labkey/api": "^1.0.0"
  }
}
